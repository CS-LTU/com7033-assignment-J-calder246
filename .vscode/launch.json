{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Flask App",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/.vscode/run.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/.vscode",
      "env": {
        "FLASK_APP": "app4.py",
        "FLASK_ENV": "development"
      }
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${fileDirname}"
    }
  ]
}
